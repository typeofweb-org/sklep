{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently --kill-others-on-fail \"yarn:dev:start:*\"",
    "dev:start:server": "wait-on tcp:5432 --interval 3000 && yarn db:migrate:up && ts-node-dev -s -H -T src/index.ts",
    "dev:start:db": "docker-compose up",
    "db:stop": "docker-compose down",
    "db:migrate:up": "prisma migrate up --experimental && prisma generate"
  },
  "dependencies": {
    "@hapi/bell": "12.1.0",
    "@hapi/boom": "9.1.0",
    "@hapi/cookie": "11.0.1",
    "@hapi/hapi": "20.0.0",
    "@hapi/inert": "6.0.2",
    "@hapi/joi": "17.1.1",
    "@hapi/vision": "6.0.1",
    "@prisma/client": "2.5.1",
    "@types/bcrypt": "3.0.0",
    "bcrypt": "5.0.0",
    "dotenv": "8.2.0",
    "hapi-swagger": "14.0.0",
    "joi": "17.2.1",
    "ms": "2.1.2"
  },
  "devDependencies": {
    "@prisma/cli": "2.5.1",
    "@tsconfig/node12": "1.0.7",
    "@types/hapi__bell": "11.0.0",
    "@types/hapi__cookie": "10.1.0",
    "@types/hapi__hapi": "20.0.0",
    "@types/hapi__inert": "5.2.0",
    "@types/hapi__joi": "17.1.4",
    "@types/hapi__vision": "5.5.1",
    "@types/ms": "0.7.31",
    "@types/node": "14.6.2",
    "ts-node-dev": "1.0.0-pre.61",
    "wait-on": "5.2.0"
  }
}
